{
  "name": "gastrobot",
  "version": "1.0.0",
  "description": "GastroBot Application",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd src/backend && npm start",
    "start:frontend": "cd src/frontend && npm start",
    "install:all": "npm install && cd src/backend && npm install && cd ../frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
} 